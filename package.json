{
  "name": "api-order",
  "version": "1.0.0",
  "description": "This is a API used for handling order request",
  "main": "./dist/server.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist && rimraf reports",
    "lint": "tslint ./src/**/*.ts ./test/**/*.spec.ts",
    "lint:fix": "tslint --fix ./src/**/*.ts ./test/**/*.spec.ts -t verbose",
    "pretest": "cross-env NODE_ENV=test npm run build && npm run lint",
    "test": "cross-env NODE_ENV=test mocha --reporter spec",
    "test:watch": "cross-env NODE_ENV=test mocha --reporter spec --require ts-node/register --watch-extensions ts --watch  test/**/*.spec.ts",
    "test:mutation": "stryker run",
    "stryker:init": "stryker init",
    "dev": "cross-env PORT=3000 NODE_ENV=dev ts-node ./src/server.ts",
    "dev:watch": "nodemon",
    "prod": "PORT=3000 npm run build && npm run start",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MagicMike90/api-order.git"
  },
  "keywords": [
    "order",
    "POC"
  ],
  "author": "Michael Luo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MagicMike90/api-order/issues"
  },
  "homepage": "https://github.com/MagicMike90/api-order#readme",
  "dependencies": {
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "chai-http": "^4.2.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-winston": "^3.1.0",
    "halson": "^3.0.0",
    "js2xmlparser": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.1",
    "mongoose-unique-validator": "^2.0.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^1.2.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.16.0",
    "@types/express-winston": "^3.0.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mocha": "^5.2.5",
    "@types/mongodb": "^3.1.23",
    "@types/mongoose": "^5.3.26",
    "@types/mongoose-unique-validator": "^1.0.1",
    "@types/node": "^11.13.4",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.1.3",
    "nodemon": "^1.18.11",
    "rimraf": "^2.6.2",
    "stryker": "^0.35.1",
    "stryker-api": "^0.24.1",
    "stryker-html-reporter": "^0.18.1",
    "stryker-mocha-framework": "^0.15.1",
    "stryker-mocha-runner": "^0.17.1",
    "stryker-typescript": "^0.18.1",
    "ts-node": "^8.0.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.1"
  }
}
