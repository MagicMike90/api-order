{
  "name": "api-order",
  "version": "1.0.0",
  "description": "This is a API used for handling order request",
  "main": "./dist/server.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist && rimraf reports",
    "lint": "tslint ./src/**/*.ts ./test/**/*.spec.ts",
    "lint:fix": "tslint --fix ./src/**/*.ts ./test/**/*.spec.ts -t verbose",
    "pretest": "cross-env NODE_ENV=test npm run build && npm run lint",
    "test": "cross-env NODE_ENV=test mocha --reporter spec --require ts-node/register test/**/*.spec.ts",
    "test:watch": "cross-env NODE_ENV=test mocha --reporter spec --require ts-node/register --watch-extensions ts --watch  test/**/*.spec.ts",
    "test:mutation": "stryker run",
    "stryker:init": "stryker init",
    "dev": "cross-env PORT=3000 NODE_ENV=dev ts-node ./src/server.ts",
    "dev:watch": "nodemon",
    "prod": "PORT=3000 npm run build && npm run start",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MagicMike90/api-order.git"
  },
  "keywords": [
    "order",
    "POC"
  ],
  "author": "Michael Luo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MagicMike90/api-order/issues"
  },
  "homepage": "https://github.com/MagicMike90/api-order#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai-http": "^4.2.1",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^1.2.0",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^11.13.4",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.1.3",
    "nodemon": "^1.18.11",
    "rimraf": "^2.6.2",
    "stryker": "^0.35.1",
    "stryker-api": "^0.24.1",
    "stryker-html-reporter": "^0.18.1",
    "stryker-mocha-framework": "^0.15.1",
    "stryker-mocha-runner": "^0.17.1",
    "stryker-typescript": "^0.18.1",
    "ts-node": "^8.0.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.1"
  }
}
